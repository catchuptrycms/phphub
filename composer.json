{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "^5.2",
        "league/html-to-markdown": "^4.2",
        "erusev/parsedown": "^1.6",
        "mews/purifier": "~2.0",
        "naux/auto-correct": "^1.0",
        "intervention/image": "^2.3",
        "roumen/sitemap": "^2.6",
        "laracasts/flash": "^2.0",
        "rap2hpoutre/laravel-log-viewer": "^0.7.0",
        "zizaco/entrust": "5.2.x-dev",
        "acoustep/entrust-gui": "5.2.x-dev",
		    "yajra/laravel-datatables-oracle": "^6.0",
        "devster/ubench": "^1.2",
        "orangehill/iseed": "^2.1",
        "predis/predis": "^1.1",
        "estgroupe/laravel-taggable": "5.1.*",
        "summerblue/administrator": "^1.0",
        "laravel/socialite": "^2.0",
        "laracasts/presenter": "^0.2.1",
        "simplesoftwareio/simple-qrcode": "^1.4",
        "thujohn/rss": "*@dev",
        "spatie/laravel-backup": "^3.8",
        "VentureCraft/revisionable": "^1.27",
        "socialiteproviders/weixin": "^2.1",
        "guzzlehttp/guzzle": "^6.2",
        "jrean/laravel-user-verification": "^2.2",
        "naux/sendcloud": "^1.1",
        "spatie/laravel-pjax": "^1.3",
        "phpdocumentor/reflection":"3.x@dev",
        "dingo/api": "1.0.x@dev",
        "jpush/jpush": "^3.2",
        "lucadegasperi/oauth2-server-laravel": "~5.0",
        "socialiteproviders/manager": "2.1.5",
        "pda/pheanstalk": "^3.1"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.2",
        "fzaninotto/faker": "~1.4",
        "laralib/l5scaffold": "1.0.4",
        "mockery/mockery": "0.9.*",
        "phpspec/phpspec": "~2.1",
        "phpunit/phpunit": "~4.0",
        "symfony/css-selector": "~3.0",
        "symfony/dom-crawler": "~3.0"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
            "App\\": "app/",
            "Phphub\\": "app/Phphub/"
        },
        "files": [
            "app/Helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "php -r \"copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "php artisan key:generate"
        ],
        "post-install-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postInstall",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "php artisan optimize"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "packages/rss"
        }
    ]
}
